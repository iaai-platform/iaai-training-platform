<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Corporate Aesthetic Training - International Aesthetic Academic Institution</title>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Montserrat', sans-serif;
            background: linear-gradient(135deg, #f8f9fa, #e9ecef);
            min-height: 100vh;
            display: flex;
            flex-direction: column;
        }

        /* Header Placeholder */
        .header-placeholder {
            height: 80px;
            background: #002147;
            display: flex;
            align-items: center;
            justify-content: center;
            color: #ffcc00;
            font-size: 18px;
            font-weight: 600;
        }

        /* Main Container */
        .main-container {
            flex: 1;
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 40px 20px;
        }

        /* Construction Card */
        .construction-card {
            background: #ffffff;
            border-radius: 25px;
            padding: 60px 40px;
            text-align: center;
            box-shadow: 0 25px 80px rgba(0, 0, 0, 0.1);
            max-width: 650px;
            width: 100%;
            position: relative;
            overflow: hidden;
        }

        .construction-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 6px;
            background: linear-gradient(90deg, #ffcc00, #b8860b, #002147);
        }

        /* Construction Icon */
        .construction-icon {
            font-size: 80px;
            color: #ffcc00;
            margin-bottom: 30px;
            animation: slideUp 2s ease-in-out infinite alternate;
        }

        @keyframes slideUp {
            0% { transform: translateY(0px); }
            100% { transform: translateY(-15px); }
        }

        /* Typography */
        .construction-card h1 {
            font-size: 36px;
            font-weight: 700;
            color: #002147;
            margin-bottom: 20px;
            line-height: 1.2;
        }

        .construction-card h2 {
            font-size: 24px;
            font-weight: 600;
            color: #ffcc00;
            margin-bottom: 25px;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .construction-card p {
            font-size: 18px;
            color: #666;
            line-height: 1.8;
            margin-bottom: 30px;
        }

        /* Coming Soon Badge */
        .coming-soon-badge {
            display: inline-block;
            background: linear-gradient(135deg, #ffcc00, #b8860b);
            color: #002147;
            padding: 12px 25px;
            border-radius: 25px;
            font-size: 14px;
            font-weight: 700;
            text-transform: uppercase;
            letter-spacing: 1px;
            margin-bottom: 30px;
            animation: shake 2s ease-in-out infinite;
        }

        @keyframes shake {
            0%, 100% { transform: translateX(0); }
            25% { transform: translateX(-5px); }
            75% { transform: translateX(5px); }
        }

        /* Action Buttons */
        .action-buttons {
            display: flex;
            gap: 20px;
            justify-content: center;
            flex-wrap: wrap;
            margin-top: 30px;
        }

        .btn {
            padding: 15px 30px;
            border-radius: 50px;
            text-decoration: none;
            font-weight: 600;
            font-size: 16px;
            transition: all 0.3s ease;
            display: inline-flex;
            align-items: center;
            gap: 10px;
        }

        .btn-primary {
            background: linear-gradient(135deg, #002147, #003366);
            color: #ffffff;
            box-shadow: 0 8px 25px rgba(0, 33, 71, 0.3);
        }

        .btn-primary:hover {
            transform: translateY(-3px);
            box-shadow: 0 12px 35px rgba(0, 33, 71, 0.5);
            background: linear-gradient(135deg, #001a3a, #002147);
        }

        .btn-secondary {
            background: transparent;
            border: 2px solid #002147;
            color: #002147;
        }

        .btn-secondary:hover {
            background: #002147;
            color: #ffffff;
            transform: translateY(-3px);
        }

        /* Features Preview */
        .features-preview {
            margin-top: 40px;
            padding-top: 30px;
            border-top: 2px dashed #e0e0e0;
        }

        .features-preview h3 {
            font-size: 20px;
            font-weight: 600;
            color: #002147;
            margin-bottom: 20px;
        }

        .features-grid {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 20px;
            margin-top: 20px;
            text-align: left;
        }

        .feature-item {
            display: flex;
            align-items: center;
            gap: 10px;
            color: #666;
            font-size: 15px;
        }

        .feature-item i {
            color: #ffcc00;
            font-size: 16px;
            width: 20px;
        }

        /* Corporate Icons */
        .corporate-icons {
            display: flex;
            justify-content: center;
            gap: 20px;
            margin: 30px 0;
            opacity: 0.3;
        }

        .corporate-icons i {
            font-size: 30px;
            color: #002147;
            animation: bob 3s ease-in-out infinite;
        }

        .corporate-icons i:nth-child(2) {
            animation-delay: 0.5s;
        }

        .corporate-icons i:nth-child(3) {
            animation-delay: 1s;
        }

        @keyframes bob {
            0%, 100% { transform: translateY(0px); }
            50% { transform: translateY(-10px); }
        }

        /* Timeline */
        .timeline {
            margin-top: 30px;
            padding-top: 20px;
            border-top: 1px solid #e0e0e0;
        }

        .timeline-item {
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 15px;
            margin: 10px 0;
            font-size: 14px;
            color: #666;
        }

        .timeline-dot {
            width: 12px;
            height: 12px;
            border-radius: 50%;
            background: #ffcc00;
            animation: pulse-dot 2s infinite;
        }

        @keyframes pulse-dot {
            0%, 100% { opacity: 1; transform: scale(1); }
            50% { opacity: 0.7; transform: scale(1.2); }
        }

        /* Responsive Design */
        @media (max-width: 768px) {
            .construction-card {
                padding: 40px 30px;
                margin: 20px;
            }

            .construction-card h1 {
                font-size: 28px;
            }

            .construction-card h2 {
                font-size: 20px;
            }

            .construction-card p {
                font-size: 16px;
            }

            .features-grid {
                grid-template-columns: 1fr;
                gap: 15px;
            }

            .action-buttons {
                flex-direction: column;
                align-items: center;
            }

            .btn {
                width: 100%;
                max-width: 250px;
                justify-content: center;
            }

            .corporate-icons {
                gap: 15px;
            }

            .corporate-icons i {
                font-size: 25px;
            }
        }

        /* Background Pattern */
        .construction-card::after {
            content: '';
            position: absolute;
            top: 50%;
            right: -50px;
            width: 100px;
            height: 100px;
            background: rgba(255, 204, 0, 0.05);
            border-radius: 50%;
            animation: expand 4s ease-in-out infinite;
            z-index: -1;
        }

        @keyframes expand {
            0%, 100% { transform: scale(1); }
            50% { transform: scale(1.5); }
        }
    </style>
</head>
<body>
    <%- include('partials/header') %>

    <main class="main-container">
        <div class="construction-card">
            <div class="construction-icon">
                <i class="fas fa-building"></i>
            </div>
            
            <div class="coming-soon-badge">
                Enterprise Solution
            </div>
            
            <h1>Corporate Aesthetic Training</h1>
            <h2>Under Construction</h2>
            
            <p>We're developing comprehensive corporate training solutions designed specifically for aesthetic clinics, medical spas, and healthcare organizations. Our enterprise-level programs will provide customized training for your entire team.</p>
            
            <div class="corporate-icons">
                <i class="fas fa-users-cog"></i>
                <i class="fas fa-chart-line"></i>
                <i class="fas fa-handshake"></i>
            </div>
            
            <div class="features-preview">
                <h3>Corporate Training Solutions:</h3>
                <div class="features-grid">
                    <div class="feature-item">
                        <i class="fas fa-users"></i>
                        <span>Team-based learning programs</span>
                    </div>
                    <div class="feature-item">
                        <i class="fas fa-calendar-alt"></i>
                        <span>Flexible scheduling options</span>
                    </div>
                    <div class="feature-item">
                        <i class="fas fa-map-marker-alt"></i>
                        <span>On-site training delivery</span>
                    </div>
                    <div class="feature-item">
                        <i class="fas fa-certificate"></i>
                        <span>Group certification programs</span>
                    </div>
                    <div class="feature-item">
                        <i class="fas fa-cog"></i>
                        <span>Customized curricula</span>
                    </div>
                    <div class="feature-item">
                        <i class="fas fa-headset"></i>
                        <span>Ongoing support & consultation</span>
                    </div>
                    <div class="feature-item">
                        <i class="fas fa-chart-bar"></i>
                        <span>Performance tracking</span>
                    </div>
                    <div class="feature-item">
                        <i class="fas fa-globe"></i>
                        <span>Multi-location coordination</span>
                    </div>
                </div>
            </div>
            
            <div class="timeline">
                <div class="timeline-item">
                    <div class="timeline-dot"></div>
                    <span><strong>Phase 1:</strong> Corporate needs assessment & program design</span>
                </div>
                <div class="timeline-item">
                    <div class="timeline-dot"></div>
                    <span><strong>Phase 2:</strong> Custom training module development</span>
                </div>
                <div class="timeline-item">
                    <div class="timeline-dot"></div>
                    <span><strong>Phase 3:</strong> Enterprise platform integration & launch</span>
                </div>
            </div>
            
            <div class="action-buttons">
                <a href="/in-company-courses" class="btn btn-primary">
                    <i class="fas fa-briefcase"></i>
                    Current In-Company Options
                </a>
                <a href="/contact-us" class="btn btn-secondary">
                    <i class="fas fa-phone"></i>
                    Discuss Corporate Needs
                </a>
            </div>
            
            <p style="margin-top: 30px; font-size: 14px; color: #999;">
                <i class="fas fa-rocket"></i>
                <strong>Enterprise-grade training</strong> requires enterprise-level preparation. We're building something exceptional.
            </p>
        </div>
    </main>

    <%- include('partials/footer') %>

    <script>
        // Enhanced interactive effects
        document.addEventListener('DOMContentLoaded', function() {
            const card = document.querySelector('.construction-card');
            const icon = document.querySelector('.construction-icon');
            
            // Card hover effect
            card.addEventListener('mouseenter', function() {
                this.style.transform = 'translateY(-10px) scale(1.02)';
                this.style.boxShadow = '0 35px 100px rgba(0, 0, 0, 0.15)';
            });
            
            card.addEventListener('mouseleave', function() {
                this.style.transform = 'translateY(0) scale(1)';
                this.style.boxShadow = '0 25px 80px rgba(0, 0, 0, 0.1)';
            });

            // Button click effects
            document.querySelectorAll('.btn').forEach(btn => {
                btn.addEventListener('click', function(e) {
                    this.style.transform = 'scale(0.95)';
                    setTimeout(() => {
                        this.style.transform = '';
                    }, 150);
                });
            });

            // Animate feature items on scroll/hover
            document.querySelectorAll('.feature-item').forEach((item, index) => {
                item.addEventListener('mouseenter', function() {
                    this.style.transform = 'translateX(10px)';
                    this.style.color = '#002147';
                });
                
                item.addEventListener('mouseleave', function() {
                    this.style.transform = 'translateX(0)';
                    this.style.color = '#666';
                });
            });

            // Timeline dots interaction
            document.querySelectorAll('.timeline-dot').forEach(dot => {
                dot.addEventListener('click', function() {
                    this.style.background = '#002147';
                    setTimeout(() => {
                        this.style.background = '#ffcc00';
                    }, 500);
                });
            });
        });
    </script>
</body>
</html>